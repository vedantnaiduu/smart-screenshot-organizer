{
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true,
    "es2022": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  },
  "ignorePatterns": ["dist/", "node_modules/", ".expo/", "**/*.ts", "**/*.tsx"]
}
