# Mobile environment variables
EXPO_PUBLIC_API_BASE=http://localhost:4000
EXPO_PUBLIC_APP_NAME="Smart Screenshot Organizer"
EXPO_PUBLIC_APP_VERSION=1.0.0

# Development
EXPO_PUBLIC_DEBUG=true
EXPO_PUBLIC_LOG_LEVEL=info

# API Configuration
EXPO_PUBLIC_API_TIMEOUT=10000
EXPO_PUBLIC_MAX_RETRIES=3

# Feature Flags
EXPO_PUBLIC_ENABLE_OCR=true
EXPO_PUBLIC_ENABLE_AUTO_TAGGING=true
EXPO_PUBLIC_ENABLE_CLOUD_SYNC=false
